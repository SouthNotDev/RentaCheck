{
  "version": 2,
  "builds": [
    {
      "src": "UI/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "outputDirectory": "UI/dist"
}